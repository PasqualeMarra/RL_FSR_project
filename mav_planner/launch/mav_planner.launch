<?xml version="1.0" encoding="UTF-8" ?>
<launch>
 
  <arg name="pln_call" default="true" />


  <include file="$(find mav_control)/launch/mav_control.launch">
    <arg name="planner_call" value="$(arg pln_call)" />
  </include>


  <rosparam file="$(find mav_planner)/conf/force_gains.yaml" command="load" />

  <node name="mav_pln" pkg="mav_planner" type="mav_planner" output="screen"/> 
 
  <include file="$(find mav_planner)/launch/aruco.launch">
    <arg name="markerId"          value="26"/>
    <arg name="markerSize"        value="0.2"/>
    <arg name="eye"               value=""/>
    <arg name="marker_frame"      value="marker_frame"/>
		<arg name="camera_frame"      value="camera_link"/>
    <arg name="ref_frame"         value=""/>
    <arg name="corner_refinement" value="HARRIS" />
  </include>


</launch>